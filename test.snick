proc main ()
    int h;
    int j;
    int k;
    int tmp;
    int x[1..8];
 
    write "Provide 8 integers: ";
    read a[1];
    read a[2];
    read a[3];
    read a[4];
    read a[5];
    read a[6];
    read a[7];
    read a[8];
    h := 1;
    while h < 8 do
        j := h;
        k := h + 1;
        while k <= n do
            if x[k] > x[j] then
                j := k;
            fi
         od
         tmp := x[h];
         x[h] := x[j];
         x[j] := tmp;
     od
     write "\nThe integers, sorted:\n\n";
     h := 1;
     while h <= 8 do
